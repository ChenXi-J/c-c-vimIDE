#ifndef GPS_H
#define GPS_H

#include <stdint.h>
#include <stdbool.h>

typedef struct {
	bool location;		//是否定位成功
	uint8_t NS;			//'N'北纬 'S'南纬
	uint8_t EW;			//'E'东经 'W'西经
	uint32_t latitude;	//纬度
	uint32_t longitude;	//经度
	uint16_t height;	//高度
	uint16_t speed;		//速度
	uint16_t direction;	//方向
	uint8_t gpsTime[6];	//采集时间
}gpsInfoStu;

void gps_init(void);

#endif
